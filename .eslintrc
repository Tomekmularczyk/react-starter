{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/prop-types": ["warn", { "ignore": ["className", "children"] }],
    "react/no-unknown-property": ["error", { "ignore": ["class", "for"] }],
    "no-underscore-dangle": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack/common.config.js"
      }
    }
  }
}
{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack.run.js --progress --env.development",
    "build:prod": "webpack --config config/webpack.run.js --progress --env.production",
    "build:server": "webpack --config config/webpack.run.js --progress --env.server",
    "build": "npm run build:prod && npm run build:server",
    "start": "node dist/server.*.bundle.js",
    "debug-ssr": "node --inspect dist/server.*.bundle.js"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.0.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "styled-jsx": "^2.1.2",
    "wicg-focus-ring": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^1.1.5",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "styled-jsx-plugin-sass": "^0.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"
  }
